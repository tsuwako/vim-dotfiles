snippet gosh
	#!/usr/bin/env gosh

snippet lambda
	(lambda (${1})
		(${2})${3})
	${4}

snippet main
	(define (main args)
		(${1})
		0)
	${2}

snippet srfi
	(use srfi-${1})
	${2}

snippet use
	(use ${1})
	${2}

snippet let
	(let ((${1:var} ${2:expr})
				${3:vars})
					${4:expr})

snippet l1
	(let1 ${1:var} ${2:expr}
		${3:expr})
